# API Configuration - Laravel Backend URL
# This should point to your Laravel backend server, typically on port 8000 or 8001
REACT_APP_API_BASE_URL=http://localhost:8001/api
REACT_APP_API_TIMEOUT=30000

# Feature Flags
REACT_APP_ENABLE_FILE_SHARING=true
REACT_APP_ENABLE_USER_SEARCH=true

# Environment
REACT_APP_ENV=development
